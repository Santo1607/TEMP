# Supabase Configuration
# Copy this file to .env.local and fill in your Supabase credentials

# Public API URL
VITE_SUPABASE_URL=https://your-project.supabase.co

# Anon (public) key for frontend
VITE_SUPABASE_ANON_KEY=your-anon-public-key-here

# Service role key (BACKEND ONLY - never expose in frontend)
# This key has admin privileges
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Database connection string (optional, for direct database access)
# Format: postgresql://user:password@db.host:5432/postgres
SUPABASE_DB_URL=postgresql://postgres:[PASSWORD]@db.your-project.supabase.co:5432/postgres

# Application environment
NODE_ENV=development

# Feature flags
ENABLE_ANALYTICS=true
ENABLE_AUDIT_LOGS=true
ENABLE_REAL_TIME=true
